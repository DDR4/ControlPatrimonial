﻿@using Microsoft.AspNet.Identity
@{
    ViewBag.Title = "Salida";
}

<section class="content-header">
    <h1>@ViewBag.Title</h1>
</section>
<section class="content">

    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">&nbsp;</h3>

                    <div class="box-tools pull-right">
                        @if (User.Identity.GetUserId().Equals("1"))
                        {
                            <button class="btn btn-success btn-sm-max" id="btnNuevo"><span class="fa fa-plus-square"></span> Nuevo @ViewBag.Title</button>
                        }

                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body ">
                    <form id="form1">
                        <div class="form-row">
                            <div class="form-group col-md-3">
                                <label for="cboTipoBusqueda">Tipo b&uacute;squeda</label>
                                <select class="form-control input-sm" style="width:100%;" id="cboTipoBusqueda">
                                    <option value="0">Todos</option>
                                    <option value="1">Código de Salida</option>
                                    <option value="2">Estado</option>
                                </select>
                            </div>
                            <div id="tipoCodigoSalida" style="display:none;">
                                <div class="form-group col-md-3">
                                    <label for="txtCodigoSalida">Código de Salida</label>
                                    <input type="text" class="form-control input-sm" id="txtCodigoSalida" maxlength="10">
                                </div>
                            </div>
                            <div id="tipoEstado" style="display:none;">
                                <div class="form-group col-md-3">
                                    <label for="cboEstado">Estado</label>
                                    <select class="form-control input-sm" id="cboEstado">
                                        <option value="0" selected>Todos</option>
                                        <option value="1">Activo</option>
                                        <option value="2">Inactivo</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-md-1">
                                <label for="btnBuscar"><font color="white">_</font></label>
                                <button type="button" class="btn btn-primary btn-sm" id="btnBuscar">
                                    <span class="glyphicon glyphicon-search"></span>&nbsp;&nbsp;Buscar
                                </button>
                            </div>
                        </div>
                    </form>
                </div>
                <br />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">&nbsp;</h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                    </div>
                </div>
                <br />
                <div class="box-body ">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="table-responsive">
                                <table id="tblListadoSalidas" class="table table-bordered table-striped table-hover table-condensed" style="width:100%;">
                                    <thead>
                                        <tr>
                                            <th width="2%;">Código</th>
                                            <th width="12%;">Asunto</th>
                                            <th width="10%;">Antecedentes</th>
                                            <th width="10%;">Analisis</th>
                                            <th width="10%;">Conclusiones</th>
                                            <th width="15%;">Recomendaciones</th>
                                            <th width="10%;">Estado</th>
                                            <th width="8%;"></th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>


<div class="modal fade" id="modalSalida" tabindex="-1" role="dialog" aria-labelledby="titleModal" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" aria-label="Close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="titleModalUsuario">Agregar @ViewBag.Title</h4>
            </div>
            <div class="modal-body">
                <form id="formModal" class="small" onkeypress="return event.keyCode != 13;">
                    <div class="row">
                        <div class="col-md-1"></div>
                        <div class="form-group col-md-3">
                            <label for="cboModalAsunto">Asunto</label>
                            <select class="form-control input-sm" id="cboModalAsunto">
                                <option value="0" selected>Todos</option>
                            </select>
                        </div>
                        <div class="form-group col-md-2">
                            <label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</label>
                            <button type="button" class="btn btn-success btn-sm" id="btnAgregarBien"><span class="fa fa-plus-square"></span>&nbsp;&nbsp;Agregar Bien</button>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-1"></div>
                        <div class="col-sm-10">
                            <div class="table-responsive">
                                <table id="tblListadoBienesSeleccionados" class="table table-bordered table-striped table-hover table-condensed" style="width:100%;">
                                    <thead>
                                        <tr>
                                            <th width="10%;">Código</th>
                                            <th width="10%;">Tipo Bien</th>
                                            <th width="10%;">Marca</th>
                                            <th width="10%;">Modelo</th>
                                            <th width="10%;">Serie</th>
                                            <th width="15%;">Fecha Vencimento Garantia</th>
                                            <th width="8%;"></th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-1"></div>
                        <div class="col-md-10">
                            <div class="form-group">
                                <label for="txtModalAntecedentes">Antecedentes</label>
                                <textarea id="txtModalAntecedentes" rows="3" cols="130" minlength="10" maxlength="255"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-1"></div>
                        <div class="col-md-10">
                            <div class="form-group">
                                <label for="txtModalAnalisis">Analisis</label>
                                <textarea id="txtModalAnalisis" rows="3" cols="130" minlength="10" maxlength="255"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-1"></div>
                        <div class="col-md-10">
                            <div class="form-group">
                                <label for="txtModalConclusiones">Conclusiones</label>
                                <textarea id="txtModalConclusiones" rows="3" cols="130" minlength="10" maxlength="255"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-1"></div>
                        <div class="col-md-10">
                            <div class="form-group">
                                <label for="txtModalRecomendaciones">Recomendaciones</label>
                                <textarea id="txtModalRecomendaciones" rows="3" cols="130" minlength="10" maxlength="255"></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-1"></div>
                        <div class="form-group col-md-3" id="divModalEstado">
                            <label for="cboModalEstado">Estado</label>
                            <select class="form-control input-sm" id="cboModalEstado">
                                <option value="0" selected>Todos</option>
                                <option value="1">Activo</option>
                                <option value="2">Inactivo</option>
                            </select>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button class="btn btn-danger btn-sm" type="button" id="btnGuardar">Guardar</button>
                <button class="btn btn-default btn-sm" type="button" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="modalBien" tabindex="-1" role="dialog" aria-labelledby="titleModal" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" aria-label="Close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Agregar Bien</h4>
            </div>
            <div class="modal-body">
                <div class="row">

                    <div class="col-md-12">
                        <div class="table-responsive">
                            <table id="tblListadoBienes" class="table table-bordered table-striped table-hover table-condensed" style="width:100%;">
                                <thead>
                                    <tr>
                                        <th width="10%;">Código</th>
                                        <th width="10%;">Tipo Bien</th>
                                        <th width="10%;">Marca</th>
                                        <th width="10%;">Modelo</th>
                                        <th width="10%;">Serie</th>
                                        <th width="15%;">Fecha Vencimento Garantia</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-danger btn-sm" type="button" id="btnGuardarBien">Guardar</button>
                <button class="btn btn-default btn-sm" type="button" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>

@section scripts{
    <script src="~/Content/js/Salida/salida.js"></script>
}